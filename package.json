    "clsx": "^1.1.1",
    "firebase": "^7.17.0",
    "firebase-admin": "9.0.0",
    "next": "latest",
    "postcss": "^8.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "swiper": "^7.3.0",
